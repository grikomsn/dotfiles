  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "better-comments.highlightPlainText": true,
  "chat.editor.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "cmake.options.statusBarVisibility": "visible",
  "color-highlight.markRuler": false,
  "cursor.composer.collapsePaneInputBoxPills": true,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.cpp.disabledLanguages": ["markdown", "plaintext", "untitled"],
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.diffs.useCharacterLevelDiffs": true,
  "debug.console.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "deno.enable": false,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.codeLensFontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "editor.experimentalInlineEdit.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "editor.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlayHints.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.largeFileOptimizations": false,
  "editor.lineHeight": 1.75,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.snippetSuggestions": "inline",
  "editor.suggest.insertMode": "replace",
  "editor.suggest.selectionMode": "always",
  "editor.suggest.showStatusBar": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "astro": "html",
    "edge": "html",
    "javascript": "javascriptreact",
    "markdown": "javascriptreact",
    "mdx": "javascriptreact",
    "rescript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": false,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.onIgnoredFiles": "warn",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "_redirects": "plaintext",
    "*.css": "tailwindcss",
    "*.mdoc": "markdown",
    "CODEOWNERS": "plaintext"
  },
  "files.exclude": {
    "**/.git": false
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.enableCommitSigning": true,
  "git.replaceTagsWhenPull": true,
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "git.useCommitInputAsStashMessage": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.completionContext.typescript.mode": "on",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.newWorkspace.useContext7": true,
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubPullRequests.experimental.chat": true,
  "githubPullRequests.experimental.useQuickChat": true,
  "githubPullRequests.pullBranch": "never",
  "gitlens.advanced.messages": {
    "suppressIntegrationDisconnectedTooManyFailedRequestsWarning": true
  },
  "gitlens.blame.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "gitlens.currentLine.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.additionalTypes": ["localBranches"],
  "gitlens.graph.minimap.enabled": false,
  "gitlens.graph.statusBar.enabled": false,
  "gitlens.home.preview.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.views.branches.avatars": false,
  "gitlens.views.branches.files.layout": "list",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commits.avatars": false,
  "gitlens.views.commits.files.layout": "tree",
  "gitlens.views.commits.pullRequests.enabled": false,
  "gitlens.views.commits.pullRequests.showForBranches": false,
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.remotes.avatars": false,
  "gitlens.views.remotes.files.layout": "tree",
  "gitlens.views.remotes.pullRequests.enabled": false,
  "gitlens.views.remotes.pullRequests.showForBranches": false,
  "gitlens.views.scm.grouped.views": {
    "branches": false,
    "commits": false,
    "contributors": false,
    "launchpad": false,
    "remotes": false,
    "repositories": false,
    "searchAndCompare": false,
    "stashes": false,
    "tags": false,
    "worktrees": false
  },
  "gitlens.views.stashes.files.layout": "tree",
  "gitlens.views.tags.avatars": false,
  "go.survey.prompt": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "makefile.configureOnOpen": true,
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "notebook.output.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "prettier.enableDebugLogs": false,
  "prettify-ts.enabled": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.remotePlatform": {
    "orb": "linux"
  },
  "scm.inputFontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "search.quickOpen.includeSymbols": true,
  "search.showLineNumbers": true,
  "security.promptForLocalFileProtocolHandling": false,
  "shellformat.useEditorConfig": true,
  "svg.preview.mode": "img",
  "tailwindCSS.emmetCompletions": true,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.confirmOnExit": "always",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.env.osx": {},
  "terminal.integrated.fontFamily": "'SF Mono Powerline', 'SF Mono', Consolas, Monaco, monospace",
  "typescript.tsserver.maxTsServerMemory": 16384,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "window.confirmBeforeClose": "always",
  "window.newWindowProfile": "Default",
  "workbench.iconTheme": "vs-seti",
  "workbench.preferredDarkColorTheme": "Large Aperture",
  "workbench.preferredLightColorTheme": "Small Aperture",
  "workbench.productIconTheme": "icons-carbon",
