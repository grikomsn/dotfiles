{
  "chakra.svg template": {
    "isFileTemplate": true,
    "prefix": "!cuisvg",
    "body": [
      "import { chakra } from \"@chakra-ui/react\";",
      "import type { ComponentProps } from \"react\";",
      "",
      "export function $1(props: ComponentProps<typeof chakra.svg>) {",
      "  return (",
      "    $0",
      "  );",
      "}",
      ""
    ]
  },
  "svg template": {
    "isFileTemplate": true,
    "prefix": "!svg",
    "body": [
      "import type { ComponentProps } from \"react\";",
      "",
      "export function $1(props: ComponentProps<\"svg\">) {",
      "  return (",
      "    $0",
      "  );",
      "}",
      ""
    ]
  },
  "Self-infer variable": {
    "isFileTemplate": false,
    "prefix": "!sif",
    "body": [
      "const ${1:variable} = (<T extends ${2:unknown}>(t: T) => t)(${TM_SELECTED_TEXT:0})"
    ]
  },
  "Log selected text to the console": {
    "isFileTemplate": false,
    "prefix": "log",
    "body": [
      "console.log(\"${0:$TM_SELECTED_TEXT}\", ${0:$TM_SELECTED_TEXT});\n"
    ]
  }
}
