[
  {
    "key": "shift+cmd+/",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+cmd+f",
    "command": "editor.action.fixAll"
  },
  {
    "key": "ctrl+alt+cmd+i",
    "command": "editor.action.organizeImports",
    "when": "textInputFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/"
  },
  {
    "key": "ctrl+shift+cmd+[",
    "command": "editor.action.selectToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+cmd+]",
    "command": "editor.action.selectToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+cmd+pagedown",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "ctrl+shift+cmd+down",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "ctrl+shift+cmd+pageup",
    "command": "editor.action.sortLinesDescending"
  },
  {
    "key": "ctrl+shift+cmd+up",
    "command": "editor.action.sortLinesDescending"
  },
  {
    "key": "alt+r",
    "command": "editor.emmet.action.updateTag"
  },
  {
    "key": "ctrl+shift+cmd+w",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "git.revertSelectedRanges",
    "when": "editorFocus || isInDiffEditor"
  },
  {
    "key": "ctrl+alt+cmd+r",
    "command": "runCommands",
    "args": {
      "commands": ["eslint.restart", "typescript.restartTsServer"]
    }
  },
  {
    "key": "ctrl+shift+cmd+backspace",
    "command": "runCommands",
    "args": {
      "commands": [
        "javascript.removeUnusedImports",
        "typescript.removeUnusedImports"
      ]
    }
  },
  {
    "key": "ctrl+alt+cmd+j",
    "command": "workbench.action.createTerminalEditor"
  },
  {
    "key": "ctrl+alt+cmd+shift+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "key": "ctrl+shift+cmd+r",
    "command": "workbench.action.restartExtensionHost"
  },
  {
    "key": "ctrl+shift+cmd+.",
    "command": "javascriptBooster.extendSelection"
  },
  {
    "key": "ctrl+shift+cmd+,",
    "command": "javascriptBooster.shrinkSelection"
  },
  {
    "key": "cmd+right",
    "command": "-editor.action.inlineSuggest.acceptNextWord",
    "when": "cppSuggestion && !editorReadonly || inlineSuggestionVisible && !editorReadonly"
  },
  {
    "key": "ctrl+shift+cmd+k",
    "command": "cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
  },
  {
    "key": "cmd+k",
    "command": "-cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
  },
  {
    "key": "ctrl+shift+cmd+k",
    "command": "aipopup.action.modal.generate",
    "when": "editorFocus && !composerBarIsVisible"
  },
  {
    "key": "shift+cmd+k",
    "command": "-aipopup.action.modal.generate",
    "when": "editorFocus && !composerBarIsVisible"
  }
]
